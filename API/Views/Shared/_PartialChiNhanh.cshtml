@{
    var listCN = API.Areas.Admin.Models.ChiNhanhCapNuoc.ChiNhanhCapNuocService.GetList();
}

<div class="box">
    <div class="header">
        <a>Các chi nhánh trực thuộc</a>
    </div>
    <div class="body white pb-0">
        @for (int i = 0; i < listCN.Count(); i++)
        {
            <div class="accordion-item mb-3">
                <h2 class="accordion-header" id="flush-heading_@listCN[i].Id">
                    <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapse_@listCN[i].Id" aria-expanded="false" aria-controls="flush-collapse_@listCN[i].Id">
                        <h5>@listCN[i].TenChiNhanh</h5>
                    </button>
                </h2>
                <div id="flush-collapse_@listCN[i].Id" class="accordion-collapse collapse chiNhanhBodyBorder" aria-labelledby="flush-heading_@listCN[i].Id" data-bs-parent="#accordionFlushExample">
                    <div class="accordion-body">
                        <h5>@listCN[i].TenChiNhanh</h5>
                        <p>@listCN[i].SoDT</p>
                        <p>@listCN[i].DiaChi</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

